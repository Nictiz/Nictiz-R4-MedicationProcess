        <rules value="open" />
      </slicing>
      <min value="3" />
    </element>
    <element id="Bundle.entry:medicationAgreement">
      <path value="Bundle.entry" />
      <sliceName value="medicationAgreement" />
      <min value="1" />
    </element>
    <element id="Bundle.entry:medicationAgreement.resource">
      <path value="Bundle.entry.resource" />
      <min value="1" />
      <type>
        <code value="Resource" />
